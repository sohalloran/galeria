<div class="gallery">
  <h1 class="gallery-title">High Art</h1>
  <h2 class="gallery-description">A display of all of your most impactful pieces.</h2>
  <h3>
    <form method="POST" enctype="multipart/form-data">
      <input type="file" name="file">
      <input type="submit" value="Upload New Art Piece">
    </form>
  </h3>
  <% @images.each do |i| %>
  <div class="gallery-item">
    <a href="/photo/<%= i.id %>">
      <span class="caption">
      </span>
    </a>
    <img src="<%= i.url %>"/>
  </div>
  <% end %>
</div>
